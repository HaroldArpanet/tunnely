# Docker image to run
TUNNELY_IMAGE=ghcr.io/your-org/tunnely:latest

# External Docker network name used by docker-compose.yml
TUNNELY_NETWORK=your-external-network

# SSH target
TARGET_IP=203.0.113.10
TARGET_PORT=22

# Local proxy ports exposed by the container
SOCKS_PORT=1080
HTTP_PROXY_PORT=8118

# Tunnel health-check
CHECK_URL=https://api.ipify.org?format=json
CHECK_INTERVAL=30
CHECK_TIMEOUT=10
